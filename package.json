{
  "name": "reactjs-sandbox",
  "version": "1.0.0",
  "license": "MIT",
  "author": "MofuMofu2",
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "dev": "vite",
    "format": "prettier -w src/*",
    "sort-json": "node scripts/sort-json.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "prepare": "husky install",
    "test": "node scripts/test.js --watchAll=false"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
