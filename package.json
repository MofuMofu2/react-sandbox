{
  "name": "reactjs-sandbox",
  "version": "1.0.0",
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:unit": "jest",
    "eject": "react-scripts eject",
    "format": "prettier -w src/*",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": "prettier --write",
    "*.tsx": "prettier --write"
  },
  "author": "MofuMofu2",
  "license": "MIT",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.6",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "jest": "^28.1.2",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "react-scripts": "5.0.1",
    "sort-by": "^1.2.0",
    "typescript": "^4.9.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/node": "^18.0.3",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.6.7",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "msw": "^1.2.1",
    "prop-types": "^15.8.1",
    "webpack": "^5.75.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
