    "msw": "^2.0.9",
    "prettier": "^3.0.3",
    "prettier-plugin-sort-json": "^3.1.0",
    "prop-types": "^15.8.1",
    "jest": "^27.4.3",
    "jest-resolve": "^27.4.2",
    "jest-watch-typeahead": "^1.0.0",
    "react-dev-utils": "^12.0.1",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0"
